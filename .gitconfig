[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  aa = add -A
  ap = add -p
  ####################
  br = branch
  brd = branch -d
  ####################
  ci = commit
  cm = commit -m
  cam = commit -am
  ####################
  co = checkout
  cob = checkout -b
  ####################
  df = diff
  ####################
  mg = merge --no-ff
  mgm = merge master
  ####################
  pl = pull
  plr = pull --rebase
  plro = pull --rebase origin
  ####################
  pu = push
  puo = push origin
  puom = push origin master
  puh = push heroku
  puhm = push heroku master
  pup = push production
  pupm = push production master
  ####################
  sa = stash
  sac = stash clear
  sal = stash list
  sap = stash pop
  sas = stash show
  ####################
  st = status
