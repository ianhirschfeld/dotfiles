[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  ci = commit
  cm = commit -m
  co = checkout
  cob = checkout -b
  df = diff
  mg = merge --no-ff
  mgm = merge origin/master
  plr = pull --rebase origin
  pu = push origin
  pum = push origin master
  st = status
  sa = stash
  sal = stash list
  sap = stash pop
